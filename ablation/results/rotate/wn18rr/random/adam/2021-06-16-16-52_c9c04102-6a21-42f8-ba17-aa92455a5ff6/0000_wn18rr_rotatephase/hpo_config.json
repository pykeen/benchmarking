{
  "type": "hpo",
  "metadata": {
    "title": "HPO Over WN18RR for RotatEPhase",
    "notes": "HPO has been divided into two runs, each running for 12 hours."
  },
  "pipeline": {
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    },
    "dataset": "wn18rr",
    "dataset_kwargs": {
      "create_inverse_triples": false
    },
    "model": "RotatE",
    "model_kwargs": {
      "automatic_memory_optimization": true
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 6,
        "high": 8,
        "scale": "power_two"
      }
    },
    "loss": "SoftplusLoss",
    "regularizer": "NoRegularizer",
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "lcwa",
    "training_kwargs": {
      "num_epochs": 1000
    },
    "training_kwargs_ranges": {
      "label_smoothing": {
        "type": "float",
        "low": 0.001,
        "high": 1.0,
        "scale": "log"
      },
      "batch_size": {
        "type": "int",
        "low": 7,
        "high": 9,
        "scale": "power_two"
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": null
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 43200,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop",
    "storage": "sqlite:////beegfs/global0/ws/meal523c-logs/rotatephase/configs/new/2021-06-16-16-52_c9c04102-6a21-42f8-ba17-aa92455a5ff6/0000_wn18rr_rotatephase/optuna_results.db"
  }
}