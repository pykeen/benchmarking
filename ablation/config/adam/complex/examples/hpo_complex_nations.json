{
  "metadata": {
    "title": "HPO Over Nations for ComplEx"
  },
  "ablation": {
    "datasets": [
      "nations"
    ],
    "models": [
      "ComplEx"
    ],
    "model_kwargs": {
      "ComplEx": {
        "automatic_memory_optimization": true
      }
    },
    "model_kwargs_ranges": {
      "ComplEx": {
        "embedding_dim": {
          "type": "int",
          "low": 10,
          "high": 15,
          "q": 1
        }
      }
    },
    "training_loops": [
      "slcwa",
      "lcwa"
    ],
    "optimizers": [
      "adam"
    ],
    "optimizer_kwargs": {
      "ComplEx": {
        "adam": {}
      }
    },
    "optimizer_kwargs_ranges": {
      "ComplEx": {
        "adam": {
          "lr": {
            "type": "categorical",
            "choices": [
              0.001,
              0.01,
              0.1,
              1.0
            ]
          }
        }
      }
    },
    "loss_functions": [
      "MarginRankingLoss"
    ],
    "loss_kwargs": {
      "ComplEx": {
        "MarginRankingLoss": {}
      }
    },
    "loss_kwargs_ranges": {
      "ComplEx": {
        "MarginRankingLoss": {
          "margin": {
            "type": "float",
            "low": 0.5,
            "high": 1.5,
            "q": 0.1
          }
        }
      }
    },
    "regularizers": [
      "NoRegularizer",
      "PowerSumRegularizer"
    ],
    "regularizer_kwargs": {
      "ComplEx": {
        "NoRegularizer": {},
        "PowerSumRegularizer": {
          "p": 2.0
        }
      }
    },
    "regularizer_kwargs_ranges": {
      "ComplEx": {
        "NoRegularizer": {},
        "PowerSumRegularizer": {
          "weight": {
            "type": "float",
            "low": 0.01,
            "high": 1.0,
            "scale": "log"
          }
        }
      }
    },
    "negative_sampler": "BasicNegativeSampler",
    "negative_sampler_kwargs": {
      "ComplEx": {
        "BasicNegativeSampler": {}
      }
    },
    "negative_sampler_kwargs_ranges": {
      "ComplEx": {
        "BasicNegativeSampler": {
          "num_negs_per_pos": {
            "type": "int",
            "low": 1,
            "high": 2,
            "q": 1
          }
        }
      }
    },
    "create_inverse_triples": [
      true
    ],
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": null
    },
    "training_kwargs": {
      "ComplEx": {
        "slcwa": {},
        "lcwa": {
          "label_smoothing": 0.0
        }
      }
    },
    "training_kwargs_ranges": {
      "ComplEx": {
        "slcwa": {
          "num_epochs": {
            "type": "int",
            "low": 5,
            "high": 10,
            "q": 100
          },
          "batch_size": {
            "type": "int",
            "low": 128,
            "high": 512,
            "q": 100
          }
        },
        "lcwa": {
          "num_epochs": {
            "type": "int",
            "low": 1,
            "high": 2,
            "q": 1
          },
          "batch_size": {
            "type": "int",
            "low": 128,
            "high": 200,
            "q": 10
          }
        }
      }
    },
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 1,
      "patience": 2
    }
  },
  "optuna": {
    "n_trials": 2,
    "timeout": 10,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "TPE",
    "pruner": "nop"
  }
}

